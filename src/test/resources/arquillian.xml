<?xml version="1.0"?>
<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://jboss.org/schema/arquillian"
	xsi:schemaLocation="http://jboss.org/schema/arquillian http://jboss.org/schema/arquillian/arquillian_1_0.xsd">
	<container qualifier="glassfish-remote" default="true">
		<configuration>
			<property name="adminUser"></property>
			<property name="adminPassword"></property>
			<property name="adminHttps">false</property>
			<property name="adminHost">localhost</property>
			<property name="adminPort">9048</property>
			<property name="target">server</property>
		</configuration>
	</container>
</arquillian>